npm install -g nodemon
npm set prefix ~/.npm
PATH="$HOME/.npm/bin:$PATH"
npm init
npm install lodash
npm install express
npm install ejs
npm install morgan
npm install mongoose
npm install --save express-session
nodemon app





--Docker--

docker run --rm -dit -e MYSQL_ROOT_PASSWORD=pass --name mysql -p 3306:3306 mysql:5.7

docker exec -it mysql bash

mysql -h 127.0.0.1 -u root -p

ALTER USER 'root'@'localhost' IDENTIFIED BY 'pass';

ALTER USER 'root'@'localhost' IDENTIFIED WITH mysql_native_password BY 'pass';


	mysql>

CREATE DATABASE matcha;

mysql --max_allowed_packet=100M -u root -p matcha

	mysql>

set global net_buffer_length=1000000;

set global max_allowed_packet=1000000000;


update user set lat=34.020882, lng=-6.841650 where username = 'wail';
update user set lat=30.427755, lng=-9.598107 where username = 'reda';
update user set lat=35.174427, lng=-6.147396 where username = 'oto';
update user set lat=31.628674, lng=-7.992047where username = 'hliwa';






IMPORTING DATABASE

mysqldump matcha > backup-file.sql
docker cp #id-container#:/backup-file.sql /Users/relkassm/Desktop/